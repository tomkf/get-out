


<div class="container">

  <h1>Hi <%= current_user.first_name.capitalize %></h1>
  <p>Please find all your bookings below</p>


  <% @bookings.each do |booking| %>
  <div>
    Title of post <%= booking.post.title %>
    Start date <%= booking.start_date %>
    End date <%= booking.end_date %>
    Price <%= booking.price %>
    Status: <%= booking.status %>
  </div>
  <br>
  <% end %>

  <h2>You have the following requests to rent you items</h2>
  <% @posts.each do |post| %>
    <% post.bookings.each do |booking| %>
      <p> <%= booking.start_date %> </p>

    <% end %>

  <% end %>

</div>


